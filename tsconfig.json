{
  "compilerOptions": {
    "target": "ES2017",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "foundry-pc-types",
      "@types/socket.io"
    ],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
